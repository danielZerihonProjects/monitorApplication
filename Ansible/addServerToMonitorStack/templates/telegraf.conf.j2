[global_tags]
[agent]
  interval = "10s"
  round_interval = true
  metric_batch_size = 1000
  metric_buffer_limit = 10000
  collection_jitter = "0s"
  flush_interval = "10s"
  flush_jitter = "0s"
  precision = ""
  quiet = false
  hostname = ""
  omit_hostname = false

[[outputs.prometheus_client]]
  listen = ":9273"
  path = "/metrics"

[[inputs.system]]
  [[inputs.exec]]
    commands = ["python3 /var/telegrafPlugins/vm_metrics.py"]
    interval = "5s"
    name_suffix = "_vm_metrics"
    data_format = "json"
    json_string_fields = ['metrics']

